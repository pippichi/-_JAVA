public class Array{
public static void main(String[] args){
int[] a={1,2};
System.out.println("Before invoking swap1:");
System.out.println("Array is {"+a[0]+" , "+a[1]+"}");
swap(a[0],a[1]);
System.out.println("After invoking swap1:");
System.out.println("Array is {"+a[0]+" , "+a[1]+"}");
System.out.println("Before invoking swap2:");
System.out.println("Array is {"+a[0]+" , "+a[1]+"}");
swapFirstTwoInArray(a[0],a[1]);
System.out.println("After invoking swap2:");
System.out.println("Array is {"+a[0]+" , "+a[1]+"}");
}
public static void swap(int n1,int n2){
int temp=n1;
n1=n2;
n2=temp;
}
public static void swapFirstTwoInArray(int[] array){
int temp=array[0];
array[0]=array[1];
array[1]=temp;
}
}